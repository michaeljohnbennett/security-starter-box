{
  "name": "@truffle/securityspeedrun",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@3.3.0",
  "engines": {
    "node": "lts/fermium"
  },
  "scripts": {
    "g:run-ganache": "ganache --wallet.defaultBalance 10000000 --wallet.deterministic --chain.allowUnlimitedContractSize",
    "g:ganache-forked": "ganache --fork mainnet --fork.blockNumber 16173000 -d -e 10000000 --chain.allowUnlimitedContractSize"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/eslint": "^8",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "ethers": "5.7.2",
    "ganache": "7.7.2",
    "truffle": "^5.7.2",
    "typescript": "4.9.3"
  }
}
